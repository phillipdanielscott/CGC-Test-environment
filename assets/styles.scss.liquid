.modalbox {
  position: absolute;
  height: 100%;
  top: -1em;
  bottom: 0;
  left: 0;
  right: 0;
  padding: 0;
  z-index:5000;
}

.overlay {
  position: fixed;
  width: 100%;
  height: 100%;
  max-height: 100%;
  top: 0;
  left: 0;
  background: #fff;
  background: rgba(255,255,255,0.95);
}

